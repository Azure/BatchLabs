{
  "name": "@batch/ui-react",
  "private": true,
  "version": "1.0.0",
  "description": "Azure Batch shared React component library",
  "repository": {
    "type": "git",
    "url": "https://msazure.visualstudio.com/DefaultCollection/One/_git/Batch-UX-Portal"
  },
  "author": "Microsoft",
  "license": "MIT",
  "main": "./dist/cjs/ui-react/index.js",
  "browser": "./dist/umd/batchreact.js",
  "test": "jest",
  "scripts": {
    "build": "run-s clean lint test compile",
    "build:clean": "run-s clean build",
    "build:dev": "run-s clean lint test compile:dev",
    "build:dev:clean": "run-s clean build:dev",
    "butil": "butil",
    "compile": "run-p compile:cjs compile:umd",
    "compile:dev": "run-p compile:cjs \"compile:umd --env.dev\"",
    "compile:cjs": "tsc --project tsconfig.build.json",
    "compile:umd": "webpack",
    "clean": "butil rmrf ./dist && butil rmrf ./test-results",
    "test": "jest",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand --watch",
    "test:watch": "jest --watch",
    "lint": "eslint",
    "watch": "run-p \"watch:*\"",
    "watch:umd": "yarn compile:umd --env.watch --env.dev",
    "watch:cjs": "yarn compile:cjs --watch --preserveWatchOutput"
  },
  "peerDependencies": {
    "@fluentui/react": ">=7.147.1 <8.0.0",
    "@fluentui/react-theme-provider": ">=0.16.2 <1.0.0",
    "@types/react": ">=16.9.35 <=17.0.0",
    "@types/react-dom": ">=16.9.8 <=17.0.0",
    "@uifabric/azure-themes": ">=7.5.19 <8.0.0",
    "react": ">=16.13.1 <17.0.0",
    "react-dom": ">=16.13.1 <17.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@batch/build-util": "1.0.0",
    "@batch/common-config": "1.0.0",
    "@batch/ui-common": "1.0.0",
    "@fluentui/react": "7.147.1",
    "@fluentui/react-theme-provider": "0.16.2",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@uifabric/azure-themes": "7.5.19",
    "npm-run-all": "^4.1.5",
    "eslint": "^7.18.0",
    "fork-ts-checker-webpack-plugin": "^4.0.4",
    "jest": "^26.6.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^7.0.5",
    "tslib": "^2.1.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.2.2",
    "webpack-sources": "^1.4.3",
    "webpackbar": "^4.0.0"
  },
  "typings": "dist/types/ui-react/index.d.ts",
  "files": [
    "dist"
  ]
}
